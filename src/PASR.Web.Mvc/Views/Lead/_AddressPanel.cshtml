@using PASR.Leads.Dto

@model AddressDto
<div class="col-12">
    <h3>Address</h3>
</div>
<div class="form-group row required">
    <label class="col-md-3 col-form-label" asp-for="@Model.FederalUnity">@L("FU")</label>
    <div class="col-md-9">
        <input type="text" class="form-control" asp-for="@Model.FederalUnity">
    </div>
</div>
<div class="form-group row required">
    <label class="col-md-3 col-form-label" asp-for="@Model.City">@L("City")</label>
    <div class="col-md-9">
        <input type="text" class="form-control" asp-for="@Model.City">
    </div>
</div>
<div class="form-group row required">
    <label class="col-md-3 col-form-label" asp-for="@Model.District">@L("District")</label>
    <div class="col-md-9">
        <input type="text" class="form-control" asp-for="@Model.District">
    </div>
</div>
<div class="form-group row required">
    <label class="col-md-3 col-form-label" asp-for="@Model.Street">@L("Street")</label>
    <div class="col-md-9">
        <input type="text" class="form-control" asp-for="@Model.Street">
    </div>
</div>
<div class="form-group row required">
    <label class="col-md-3 col-form-label" asp-for="@Model.Number">@L("Number")</label>
    <div class="col-md-9">
        <input type="text" class="form-control" asp-for="@Model.Number">
    </div>
</div>